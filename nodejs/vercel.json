{
  "version": 2,
  "framework": "vite",
  "installCommand": "pnpm install",
  "devCommand": "vite",
  "buildCommand": "pnpm run build",
  "builds": [
    {
      "src": "./dist/_worker.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/_worker.js"
    }
  ]
}
