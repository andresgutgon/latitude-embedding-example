{
  "version": 2,
  "framework": "vite",
  "installCommand": "pnpm install",
  "devCommand": "vite",
  "buildCommand": "pnpm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "honox"
    },
    {
      "src": "app/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/_worker.js"
    }
  ]
}
